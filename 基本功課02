#include <stdio.h> //函示庫

#include <stdlib.h> //函示庫



int main() 

{

 int y,m,d; //定義年,月,日 

 printf("請輸入你的出生年分(西元)：");

 scanf("%d",&y);  //存年份 

 printf("請輸入你的出生月分：");

 scanf("%d",&m); //存月份 

 printf("請輸入你的出生日期：");

 scanf("%d",&d);  //存日期 

 printf("你的出生年月日為：%d,%d,%d\n",y,m,d);

 system("pause");   

 return 0;

}
